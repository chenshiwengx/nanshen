    </plugins> -->
      <typeAliases>  
        <typeAlias alias="User1" type="com.bolo.entity.User1"/>  
        <typeAlias alias="Location" type="com.bolo.entity.Location"/> 
        <typeAlias alias="Wifiinfo" type="com.bolo.entity.Wifiinfo"/> 
        <typeAlias alias="Wifiinfoavg" type="com.bolo.entity.Wifiinfoavg"/> 
