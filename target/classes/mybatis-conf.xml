    <mappers>  
        <mapper resource="com/bolo/dao/UserServiceMapper.xml"/>  
        <mapper resource="com/bolo/dao/LocationMapper.xml"/> 
        <mapper resource="com/bolo/dao/WifiinfoMapper.xml"/>
